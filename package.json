{
  "name": "nikkovote",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "migrations:generate": "drizzle-kit generate",
    "migrations:apply:local": "wrangler d1 migrations apply nikkovote --local",
    "migrations:apply:remote": "wrangler d1 migrations apply nikkovote --remote"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240924.0",
    "@fontsource/fira-mono": "^5.1.0",
    "@neoconfetti/svelte": "^2.2.1",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/adapter-cloudflare": "^4.7.3",
    "@sveltejs/kit": "^2.5.28",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.24.2",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "wrangler": "^3.78.8"
  },
  "type": "module",
  "dependencies": {
    "drizzle-orm": "^0.33.0",
    "zod": "^3.23.8"
  }
}
